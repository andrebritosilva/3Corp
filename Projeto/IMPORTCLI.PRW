User Function ImpCliCSV(cArquivo, lEnd)

Local aVetor    := {}
Local aDados    := {}
Local aArea		:= GetArea()
Local cLinha
Local nTot      := 0
Local nCont     := 1
Local nAtual    := 0
Local nTimeIni  := 0
Local nLinTit   := 1  // Total de linhas do Cabeçalho
Local aCampos 	:= {} 
Local cCgc		:= ""
Local cCodigo	:= ""
Local cLoja		:= ""
Local lGrava	:= .T.
Local nPosNom  	:= 0
Local nPosCgc   := 0
Local nPosCod   := 0
Local nPosDes   := 0
Local nPosLoj	:= 0
Local cTipo		:= ""
Local cNomArqErro := ""
Local cErroTemp	:= ""
Local nLinhas	:= 0
Local cBuffer	:= ""
Local cCampo	:= ""
Local cDescErro	:= ""
Local cConteudo	:= ""
Local nErrLin	:= 0
Local lOpErro	:= .F.
Local lErro		:= .F.
Local nHandle	:= 0
Local X			:= 1

Private lMsErroAuto := .F.
Private cPath       := SubStr(cArquivo,1,RAT("\", cArquivo))

If (nHandle := FT_FUse(AllTrim(cArquivo)))== -1
	Help(" ",1,"NOFILEIMPOR")
	RestInter()
	Return .F.
EndIf

nTot := FT_FLASTREC()

FT_FGOTOP()

// Tratamento do cabeçalho
While nLinTit > 0 .AND. !Ft_FEof()
   cLinha := FT_FREADLN()

   If LEN(cLinha) == 1023
		FT_FSKIP()
		cConLinha := FT_FREADLN()

		While LEN(cConLinha) == 1023
			cLinha += cConLinha
			FT_FSKIP()
			cConLinha := FT_FREADLN()
		EndDo

		cLinha += cConLinha

	EndIf

	If nLinTit == 1
		aCampos := SEPARA(cLinha,";",.T.)
		cLinha += ";DESC. ERRO"
	EndIf

	cLinha := ""
	Ft_FSkip()
	nLinTit--
	
EndDo

oProcess:SetRegua1( nTot )
oProcess:SetRegua2( int(ntot/100) )

// Processa os dados do template
Do While !FT_FEOF()
	oProcess:IncRegua1("Registros processados : " + ALLTRIM(STR(nCont)) )
	aVetor := {}
	cLinha := FT_FREADLN()
	
	If lEnd
		MsgInfo("Importação cancelada!","Fim")
		Return .F.
	Endif

	nAtual++
	If (nAtual % 100) = 1
		nTimeIni := Seconds()
	EndIf
	If (nAtual % 100) = 0
		oProcess:IncRegua2( "Tempo Restante - (" + EstTime(ntot,nAtual,(nAtual-100),nTimeIni) + ")" )
	EndIf
	
	If LEN(cLinha) == 1023
		FT_FSKIP()
		cConLinha := FT_FREADLN()
		While LEN(cConLinha) == 1023
			cLinha += cConLinha
			FT_FSKIP()
			cConLinha := FT_FREADLN()
		EndDo
		cLinha += cConLinha
	EndIf

	nAtual++
	If (nAtual % 100) = 1
		nTimeIni := Seconds()
	EndIf
	If (nAtual % 100) = 0
		oProcess:IncRegua2( "Tempo Restante - (" + EstTime(ntot,nAtual,(nAtual-100),nTimeIni) + ")" )
	EndIf
	
	aDados := SEPARA(cLinha,";",.T.) // A função SEPARA e a função STRTOKARR, converte uma string em um array, o SEPARA converte os espaços em branco 

	For X:=1 To LEN(aCampos)

		SX3->(dbSetOrder(2)) 
		SX3->(dbGoTop())

		If SX3->(dbSeek(aCampos[X]))
			cTipo := SX3->X3_TIPO
		EndIf

		Do Case
			Case cTipo == 'D' 
				aDados[X] := CTOD(aDados[X])
			Case cTipo == 'N' 
				aDados[X] := IIf(Empty(aDados[X]),0,VAL(STRTRAN(aDados[X],",",".")))
			Case cTipo == 'M' 
				aDados[X] := IIf(Empty(aDados[X]),"",MSMM(aDados[X]))
		EndCase
			
		If AllTrim(aCampos[X]) == "A1_COD"
				
			If Empty(aDados[X])
				
				aDados[X]:= GETSXENUM("SA1","A1_COD")
				
			EndIf
			
		EndIf

		If AllTrim(aCampos[X]) == "A1_LOJA"
				
			If Empty(aDados[X])
				
				aDados[X]:= "0001"
			
			EndIf
			
		EndIf

		If AllTrim(aCampos[X]) == "A1_CGC"
				
			cCGC:= AllTrim(aDados[X])
				
		EndIf
		
		If !Empty(aDados[X])
				
			AADD(aVetor,{aCampos[X],aDados[X] ,Nil})
				
		EndIf

	Next X
	
	SA1->(DbSetOrder(3)) // A1_FILIAL+A1_CGC
	SA1->(DbGoTop())	
	
	lGrava:= .T.

	If SA1->(DbSeek(xFilial("SA1") + AllTrim(cCgc) ) )

		nPosNom  := aScan(aCampos,{ |x| Upper(AllTrim(x)) == "A1_NOME" })
		nPosCgc  := aScan(aCampos,{ |x| Upper(AllTrim(x)) == "A1_CGC" })
		nPosCod  := aScan(aCampos,{ |x| Upper(AllTrim(x)) == "A1_COD" })
		nPosDes  := aScan(aCampos,{ |x| Upper(AllTrim(x)) == "A1_LOJA" })
		
		AADD(aLog,{"A1_COD"  ,aDados[nPosCod] ,Nil}) // Codigo
		AADD(aLog,{"A1_LOJA" ,aDados[nPosDes] ,Nil}) // Loja
		AADD(aLog,{"A1_NOME" ,aDados[nPosNom] ,Nil}) // Nome
		AADD(aLog,{"A1_CGC"  ,aDados[nPosCgc] ,Nil}) // CNPJ/CPF
		AADD(aLog,{"ERRO" 	 ,"Cliente já cadastrado - A1_CGC" ,Nil}) // Descrição
		                                              
		lGrava:= .F.
		
		RollBackSx8()

	EndIf

	cCodigo:= ""
	cLoja:= ""

	If SA1->(DbSeek(xFilial("SA1") + AllTrim(SubStr(cCgc,1,8)) ) ) .And. lGrava 

		cCodigo	:= SA1->A1_COD
		cLoja	:= SA1->A1_LOJA
	
		SA1->(DbSetOrder(1))
		SA1->(DbGoTop())
	
		While SA1->(DbSeek(xFilial("SA1") + cCodigo + cLoja ) )
			cLoja := Strzero((Val(cLoja) + 1),4)
		End
  		
  		nPosLoj  := aScan(aVetor,{ |x| x[1] == "A1_LOJA" })
  		aVetor[nPosLoj][2]:= cLoja
  		
  		nPosCod  := aScan(aVetor,{ |x| x[1] == "A1_COD" })
  		aVetor[nPosCod][2]:= cCodigo

	EndIf

	SA1->(DbSetOrder(1))
	
	If lGrava
	
		MSExecAuto({|x,y| Mata030(x,y)},aVetor,3) //3- Inclusão, 4- Alteração, 5- Exclusão

	EndiF
			
	If lMsErroAuto .And. lGrava
	
		cNomArqErro := "ARQLOG.LOG"
		cErroTemp := Mostraerro(cPath, cNomArqErro) 
		nLinhas   := MLCount(cErroTemp) 
		cBuffer   := "" 
		cCampo    := "" 
		cDescErro := ""
		cConteudo := ""
		nErrLin   := 1 
		lOpErro   := .T.
		lErro     := .T.
		cBuffer   := RTrim(MemoLine(cErroTemp,,nErrLin))
		//Carrega o nome do campo 
		While (nErrLin <= nLinhas)
			nErrLin++
			cBuffer:=RTrim(MemoLine(cErroTemp,,nErrLin))
			If lErro
				If ALLTRIM(cBuffer) <> ""
					cDescErro += cBuffer+" "
				Else
					lErro := .F.
				EndIf
			EndIf
			If (Upper(SubStr(cBuffer,Len(cBuffer)-7,Len(cBuffer))) == "INVALIDO")
				cCampo    := cBuffer
				Exit
			EndIf 
		EndDo
		
		FERASE(cPath + cNomArqErro)
	
		nPosNom := aScan(aCampos,{ |x| Upper(AllTrim(x)) == "A1_NOME" })
		nPosCgc := aScan(aCampos,{ |x| Upper(AllTrim(x)) == "A1_CGC" })
		nPosCod := aScan(aCampos,{ |x| Upper(AllTrim(x)) == "A1_COD" })
		nPosDes := aScan(aCampos,{ |x| Upper(AllTrim(x)) == "A1_LOJA" })
		
		AADD(aLog,{"A1_COD"  ,aDados[nPosCod] ,Nil}) // Codigo
		AADD(aLog,{"A1_LOJA" ,aDados[nPosDes] ,Nil}) // Loja
		AADD(aLog,{"A1_NOME" ,aDados[nPosNom] ,Nil}) // Nome
		AADD(aLog,{"A1_CGC"  ,aDados[nPosCgc] ,Nil}) // CNPJ/CPF
		AADD(aLog,{"ERRO" 	 ,cDescErro+" - "+cCampo ,Nil}) // Descrição
		
		lMsErroAuto := .F.
		
		RollBackSx8()
		
	ElseIf lGrava
		
		ConfirmSX8()
	
	Endif
	
	FT_FSKIP()
	nCont++
EndDo

FT_FUSE()

Aviso("Finalizado","Importacao Finalizada - " + IIF(Empty(aLog),"Com Sucesso!!!	", "Verifique Arquivo de Log"),{"Fechar"})

RestArea(aArea)

Return .T.

